generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model About {
  id      String  @id @default(auto()) @map("_id") @db.ObjectId
  enabled Boolean
  name_en String
  name_fr String
  shown   Boolean
  url     String
}

model Event {
  id      String  @id @default(auto()) @map("_id") @db.ObjectId
  enabled Boolean
  name_en String
  name_fr String
  shown   Boolean
  url     String
}

model General {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Home {
  id              String @id @default(auto()) @map("_id") @db.ObjectId
  all_events_url  String
  discord_url     String
  mail            String
  show_twitter_id String
  show_youtube_id String
  tiktok_url      String
  twitch_url      String
  twitter_url     String
  youtube_url     String
}

model Network {
  id              String @id @default(auto()) @map("_id") @db.ObjectId
  all_events_url  String
  discord_url     String
  mail            String
  show_twitter_id String
  show_youtube_id String
  tiktok_url      String
  twitch_url      String
  twitter_url     String
  youtube_url     String
}

model Team {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  logo    String
  /// Could not determine type: the field only had null or empty values in the sample set.
  members Json?
  name    String
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Match {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  equipe_1 String
  equipe_2 String
  score_1  Int
  score_2  Int
  date     DateTime
}
